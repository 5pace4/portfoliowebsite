{
  "rewrites": [
    {
      "source": "/gallery/:path*",
      "destination": "https://photo-gallery-tofayel.vercel.app/:path*"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://tofayelahmmedbabu.blogspot.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/gallery/download-image",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ]
}
